{
    "version": 2,
     "rewrites": [
        { "source": "/(.*)", "destination": "src/api" }
    ],
    "builds": [
        {
            "src": "src/api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/api/index.ts"
        }
    ]
}